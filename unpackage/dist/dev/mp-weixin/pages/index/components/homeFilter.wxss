/* 水平间距 */
body,
uni-page-head,
.uni-page-head {
  background-color: var(--neutral-color-white) !important;
}
.homePageBox {
  position: relative;
  padding-top: 430rpx;
  overflow: hidden;
}
.homePageBox .boxBg {
  box-shadow: 0 0 22rpx 22rpx rgba(162, 162, 162, 0.06);
}
.scrollList {
  padding-bottom: 130rpx;
  height: calc(100vh - 180rpx);
}
.homeFilter {
  background-color: var(--neutral-color-white);
  height: 100rpx;
  margin-top: 74rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: relative;
  z-index: 9999;
}
.homeFilter .leftBox {
  height: 60rpx;
  width: 180rpx;
  background-color: var(--color-gray-e9);
  border-radius: 8rpx;
  display: flex;
  line-height: 60rpx;
  font-size: 28rpx;
  color: var(--color-gray-59);
  justify-content: space-between;
  margin-left: 30rpx;
}
.homeFilter .item:first-child {
  margin-left: 20rpx;
}
.homeFilter .item:last-child {
  margin-right: 20rpx;
}
.homeFilter .item.active {
  color: var(--color-red-bg);
  background-color: var(--neutral-color-white);
  margin: 6rpx 0rpx 6rpx 6rpx;
  border-radius: 8rpx;
  width: 84rpx;
  height: 48rpx;
  text-align: center;
  line-height: 48rpx;
}
.homeFilter .item:last-child.active {
  margin: 6rpx 6rpx 6rpx 0rpx;
}
.homeFilter .rightBox {
  display: flex;
  align-items: center;
}
.homeFilter .rightBox .label {
  font-size: 28rpx;
  color: var(--color-gray-59);
}
.homeFilter .rightBox .icon {
  height: 16rpx;
  width: 16rpx;
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center;
  margin-right: 32rpx;
  margin-left: 16rpx;
}
.homeFilter .rightBox .down {
  background-image: url("data:image/png;base64,");
}
.homeFilter .rightBox .up {
  background-image: url("data:image/png;base64,");
}
.homeFilter .filterPopup {
  background-color: rgba(0, 0, 0, 0.25);
  position: absolute;
  width: 100%;
  height: 100vh;
  top: 90rpx;
  z-index: 999;
}
.homeFilter .filterPopup .service {
  display: flex;
  background-color: var(--neutral-color-white);
  border-top: 2rpx solid var(--color-gray-e8);
  justify-content: space-between;
  padding: 0 29rpx;
  flex-wrap: wrap;
  padding-top: 41rpx;
}
.homeFilter .filterPopup .service .serviceItem {
  text-align: center;
  line-height: 64rpx;
  height: 64rpx;
  width: 216rpx;
  border-radius: 32rpx;
  background-color: var(--color-gray-f2);
  margin-bottom: 41rpx;
}
.homeFilter .filterPopup .service .serviceItem.active {
  height: 60rpx;
  width: 212rpx;
  line-height: 60rpx;
  border: 2rpx solid var(--color-red-bg);
  border-radius: 32rpx;
  color: var(--color-red-bg);
  background-color: var(--color-red-ffeaea);
}
.homeFilter .filterPopup .footer {
  display: flex;
  background-color: var(--neutral-color-white);
  height: 146rpx;
  border-radius: 0 0 16rpx 16rpx;
  justify-content: space-between;
  border-top: 2rpx solid var(--color-gray-e8);
}
.homeFilter .filterPopup .footer .btn {
  height: 88rpx;
  border-radius: 50px;
  line-height: 88rpx;
  text-align: center;
  margin-top: 28rpx;
}
.homeFilter .filterPopup .footer .btn-red {
  width: 408rpx;
  margin-right: 30rpx;
}
.homeFilter .filterPopup .footer .btn-gray {
  width: 250rpx;
  margin-left: 30rpx;
}
.homeFilter.fixTop {
  position: fixed;
  top: 50rpx;
  z-index: 9999;
  margin-top: 0rpx;
  width: 100%;
}
.homeFilter.fixTop .filterPopup {
  position: absolute;
  top: 100rpx;
}
.homeList .card {
  border-radius: 16px;
  background-color: var(--neutral-color-white);
  min-height: 270rpx;
  margin: 30rpx;
  padding: 30rpx 30rpx 0rpx 30rpx;
}
.homeList .card .card-content {
  display: flex;
  margin-bottom: 23rpx;
}
.homeList .card .card-content .leftCardContent {
  width: 140rpx;
  height: 114rpx;
  border-radius: 6px;
  margin-right: 28rpx;
}
.homeList .card .card-content .rightCardContent .title {
  font-size: 32rpx;
  font-weight: bold;
  color: var(--color-black-19);
  margin-bottom: 18rpx;
}
.homeList .card .card-content .rightCardContent .serviceTime, .homeList .card .card-content .rightCardContent .serviceAddress {
  font-size: 28rpx;
}
.homeList .card .card-content .rightCardContent .serviceTime text:first-child, .homeList .card .card-content .rightCardContent .serviceTime view:first-child, .homeList .card .card-content .rightCardContent .serviceAddress text:first-child, .homeList .card .card-content .rightCardContent .serviceAddress view:first-child {
  color: var(--color-gray-93);
  margin-right: 16rpx;
  white-space: nowrap;
}
.homeList .card .card-content .rightCardContent .serviceTime text:last-child {
  color: var(--color-black-19);
}
.homeList .card .serviceAddress {
  display: flex;
  margin-top: 10rpx;
}
.homeList .card .serviceAddress .address .addressContent {
  color: var(--color-black-19);
  white-space: pre-wrap;
  font-size: 28rpx;
  line-height: 32rpx;
  margin-right: 0rpx;
  margin-bottom: 19rpx;
}
.homeList .card .cardFooter {
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-top: 2rpx solid var(--color-gray-e8);
  height: 95rpx;
}
.homeList .card .cardFooter .price {
  font-size: 36rpx;
  font-weight: bold;
  color: var(--color-red-bg);
}
.homeList .card .cardFooter .price .price-label {
  font-size: 28rpx;
  font-weight: normal;
  color: var(--color-gray-59);
  margin-right: 22rpx;
}
.homeList .card .cardFooter .robBtn {
  font-size: 24rpx;
  text-align: center;
  line-height: 56rpx;
  width: 146rpx;
  height: 56rpx;
  border-radius: 32rpx;
}
.homeList .footer {
  font-size: 24rpx;
  color: var(--color-gray-9);
  text-align: center;
}